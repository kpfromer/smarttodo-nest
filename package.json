{
  "name": "smarttodo-nest",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:dev": "nodemon",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kpfromer/smarttodo-nest.git"
  },
  "author": "Kyle Pfromer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kpfromer/smarttodo-nest/issues"
  },
  "homepage": "https://github.com/kpfromer/smarttodo-nest#readme",
  "dependencies": {
    "@nestjs/common": "^5.0.0-beta.3",
    "@nestjs/core": "^5.0.0-beta.3",
    "mongoose": "^5.0.16",
    "nest-router": "^1.0.5",
    "nestjs-typegoose": "^1.0.3",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "typegoose": "^5.0.0",
    "confidence": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^9.6.6",
    "@types/jest": "^22.2.3",
    "coveralls": "^3.0.0",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.4",
    "typescript": "^2.8.3",
    "ts-node": "^6.0.0",
    "tsconfig-paths": "^3.3.1",
    "@types/confidence": "^1.4.30"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "resetMocks": true
  }
}
