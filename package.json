{
  "name": "smarttodo-nest",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "now-build": "rm -rf dist && tsc",
    "now-start": "node dist/main.js",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:dev": "nodemon",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kpfromer/smarttodo-nest.git"
  },
  "author": "Kyle Pfromer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kpfromer/smarttodo-nest/issues"
  },
  "homepage": "https://github.com/kpfromer/smarttodo-nest#readme",
  "dependencies": {
    "@nestjs/common": "^5.0.1",
    "@nestjs/core": "^5.0.0",
    "@nestjs/passport": "^1.0.11",
    "@nestjs/testing": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.8.5",
    "confidence": "^3.0.2",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.1.1",
    "nest-router": "^1.0.6",
    "nestjs-typegoose": "^5.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.1.0",
    "typegoose": "^5.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/confidence": "^1.4.30",
    "@types/jest": "^22.2.3",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/mongoose": "^5.0.13",
    "@types/node": "^10.0.8",
    "@types/passport": "^0.4.5",
    "@types/supertest": "^2.0.4",
    "coveralls": "^3.0.1",
    "jest": "^22.4.3",
    "supertest": "latest",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.0.3",
    "tsconfig-paths": "^3.3.2",
    "typescript": "^2.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "resetMocks": true
  }
}
